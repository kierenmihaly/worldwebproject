import java.util.Scanner;

public class  {
    public static void main(String[] args) {
        String name;          // 이름
        String number;      // 전화번호
       
        Scanner input = new Scanner(System.in);    // 사용자로부터 값을 입력받음
       
       
        System.out.print("이름: ");     // 이름 출력
        name = input.next();             // 이름을 사용자로부터 입력받음

        System.out.print("전화번호: ");     // 전화번호 출력
        number = input.next();                // 전화번호를 사용자로부터 입력받음

       
        System.out.println("=====================================");
        System.out.println("[회원정보]");
        System.out.print( "이름은 " + name +  "전화번호는 " + number + "\n"); // 총합적인 회원정보를 출력
    }
}





}


