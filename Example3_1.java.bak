public class Example3_1 {
 
    public static void main(String[] args) {
 
        int i = 1;        
        int sum = 0;
 
        while (i < 11) {
           
            sum = sum + i;
                 i++;
        }

        System.out.println("10부터 1까지의 합은 " + sum + "입니다.");
 
    }//main
 
}//class
